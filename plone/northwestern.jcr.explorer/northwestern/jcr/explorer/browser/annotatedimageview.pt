<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="northwestern.jcr.explorer">
<body>

<div metal:fill-slot="main">
  <tal:main-macro metal:define-macro="main"
  tal:define="kssClassesView context/@@kss_field_decorator_view;
  getKssClasses nocall:kssClassesView/getKssClassesInlineEditable;
  ">

<div tal:replace="structure provider:plone.abovecontenttitle" />


  <h1 class="documentFirstHeading"> 
  <metal:field use-macro="python:here.widget('title', mode='view')">
  Title
  </metal:field>
  </h1>

  <p class="Description">
  <span tal:define="description python:here.description" tal:content="description">
  Description
  </span>
  </p>

  <span tal:define="description python:here.bounds" tal:content="description">
  Description
  </span>

<div tal:replace="structure provider:plone.abovecontentbody" />

	<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,0,0" width="100%%" height="500" id="tree-view" align="middle" tal:condition="here/image">
		<param name="allowScriptAccess" value="sameDomain" />
		<param name="movie" tal:attributes="value string:viewer2.swf?image=${here/image}&bounds=${here/bounds}" value="viewer.swf" />
		<param name="quality" value="high" />
		<param name="scale" value="noscale" />
		<param name="salign" value="lt" />
		<param name="bgcolor" value="#ffffff" />
		<embed src="viewer.swf" tal:attributes="src string:viewer2.swf?image=${here/image}&bounds=${here/bounds}" quality="high" scale="noscale" salign="lt" bgcolor="#ffffff" width="100%%" height="500" name="tree-view" align="middle" allowscriptaccess="sameDomain" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" />
	</object>
  
<div metal:use-macro="here/document_relateditems/macros/relatedItems">
  show related items if they exist
</div>

<div tal:replace="structure provider:plone.belowcontentbody" />

  </tal:main-macro>
</div>
	
	
</body>
</html>
